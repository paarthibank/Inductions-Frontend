	<script type="text/javascript">
	fetch("https://api.nytimes.com/svc/topstories/v2/arts.json?api-key=rejivPChi0SenNSeddJInndI8FJIX8lu", {
			"method": "GET",
			
		})
		.then(response => {
			response.json()
			.then(data=>{
				success(data);
			});
		})
		.catch(err => {
			alert(err);
		});
		function success(data){
			var img=document.getElementsByClassName("img");
			var details=document.getElementsByClassName("details");
			var head=document.getElementsByClassName("head");
			var content=document.getElementsByClassName("content");
			for(i=0;i<34;i++){
				head[i].innerHTML=data.results[i].title;
				content[i].innerHTML=data.results[i].abstract;
				details[i].href=data.results[i].url;
				img[i].src=data.results[i].multimedia[0].url;
				
			}
		}
	</script>


